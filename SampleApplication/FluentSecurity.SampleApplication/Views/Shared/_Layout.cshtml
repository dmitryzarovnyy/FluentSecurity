<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta http-equiv="content-language" content="en" />
    <title>TitleContent</title>
    <meta name="copyright" content="Copyright (c) 2009 Spinit AB" />
    <meta name="author" content="Spinit AB" />
    @RenderSection("Styles", required: false)
    <link rel="stylesheet" href="~/css/site.css" type="text/css" />
</head>
<body>
    <div id="Wrapper">
        <div id="Header">
            <div id="Branding">Fluent Security sample app</div>
            <ul id="Main-Navigation">
                <li><a href="/">Home</a></li>
            </ul>
        </div>
        <div id="Content">
            <div id="Main-Content">
                <asp:ContentPlaceHolder ID="MainContent" runat="server" />
                @RenderBody()
            </div>
        </div>

        <div id="Footer" class="vcard">
            <a href="http://www.spinit.se/" class="fn url">Kristoffer Ahl</a>, <a href="mailto:fluent-security@77dynamite.com" class="email">fluent-security@77dynamite.com</a>
        </div>
    </div>
    
</body>
</html>